<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <padding
        android:left="25dp"
        android:top="25dp"
        android:right="25dp"
        android:bottom="25dp"/>
    <size
        android:width="50dp"
        android:height="50dp"/>
    <solid
        android:color="@color/Do"/>
    <stroke
        android:width="1dp"
        android:color="#00EB09"
        android:dashWidth="90dp"
        />
    <gradient
        android:angle="270"
        android:centerColor="#0F1EE2"
        android:endColor="#EDF0FA"
        android:startColor="#573F3F" />
    <corners
        android:radius="5dp"/>
</shape>