<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <padding
        android:left="25dp"
        android:top="25dp"
        android:right="25dp"
        android:bottom="25dp"/>
    <size
        android:width="50dp"
        android:height="50dp"/>
    <solid
        android:color="@color/Do"/>
    <stroke
        android:width="5dp"
        android:color="@color/Vang"
        android:dashWidth="90dp"
        />
    <gradient
        android:angle="270"
        android:centerColor="#6E7FFF"
        android:endColor="#142FFC"
        android:startColor="#BAC2FF" />
    <corners
        android:radius="5dp"/>
</shape>